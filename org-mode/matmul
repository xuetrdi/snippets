\begin{displaymath}
  \left[
    \begin{matrix}
      $1
    \end{matrix}
  \right]
  \times
  \left[
    \begin{matrix}
      $2
    \end{matrix}
  \right]
  =
  \left[
    \begin{matrix}
      $3
    \end{matrix}
  \right]
\end{displaymath}
