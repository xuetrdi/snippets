\begin{displaymath}
  \left[
    \begin{matrix}
      $1
    \end{matrix}
  \right]
  \div
  \left[
    \begin{matrix}
      $2
    \end{matrix}
  \right]
  =
  \left[
    \begin{matrix}
      $3
    \end{matrix}
  \right]
\end{displaymath}
